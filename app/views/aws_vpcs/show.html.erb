<%- model_class = AwsVpc -%>
<div class="page-header">
  <h1><%=t '.title', :default => AwsVpc %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @aws_vpc.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:aws_name) %></dt>
      <dd><pre class="prettyprint"><%= @aws_vpc.aws_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cidr_block) %></dt>
      <dd><pre class="prettyprint"><%= @aws_vpc.cidr_block %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:instance_tenancy) %></dt>
      <dd><pre class="prettyprint"><%= @aws_vpc.instance_tenancy %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:enable_dns_support) %></dt>
      <dd><pre class="prettyprint"><%= @aws_vpc.enable_dns_support %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:enable_dns_hostnames) %></dt>
      <dd><pre class="prettyprint"><%= @aws_vpc.enable_dns_hostnames %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:subnet_base) %></dt>
      <dd><pre class="prettyprint"><%= @aws_vpc.subnet_base %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              aws_vpcs_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_aws_vpc_path(@aws_vpc), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              aws_vpc_path(@aws_vpc),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

